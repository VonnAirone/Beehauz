{
  "expo": {
    "name": "Beehauz",
    "slug": "beehauzapp",
    "version": "1.0.0",
    "scheme": "com.beehauz",
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],[
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Beehauz to use your location."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Beehauz to access your photos.",
          "savePhotosPermission": "Allow Beehauz to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Beehauz needs access to your Camera.",
          // "enableMicrophonePermission": true,
          // "microphonePermissionText": "Beehauz needs access to your Microphone."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aironevonn.beehauzapp"
    },
    "android": {
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash.png"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.aironevonn.beehauzapp",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAXn-flhdb__4Fqa-4pbPJsXk9DzF2ScPo"
        }
      }
     
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7238d136-11d1-4ca7-9c76-f502360e6cc8"
      }
    },
    "owner": "aironevonn"
  }
}
